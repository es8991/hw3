<h1><%= @place["name"] %></h1>
<h2>My Entries</h2>

<ul>

<% for entry in @entries %>
  <li><%= entry["title"] %> 
    <p><%= entry["occurred_on"] %></p>
    <p><%= entry["description"] %></p>
  </li>
<% end %>

</ul>

<p>
  <a href="/places">Back to Places</a>
</p>

<p>
  <a href="/places/<%= @place["id"] %>/entries/new">New Entry</a>
</p>


