<h1>New Entry for <%= @place.name %></h1>

<form action="/places/<%= @place.id %>/entries" method="post">
  <p>
    <label for="title">Title</label>
    <input type="text" name="entry[title]" id="title">
  </p>

  <p>
    <label for="occurred_on">Occurred On</label>
    <input type="date" name="entry[occurred_on]" id="occurred_on">
  </p>

  <p>
    <label for="description">Description</label>
    <textarea name="entry[description]" id="description"></textarea>
  </p>

  <button>Submit</button>
</form>